<div>
 <div id = "comedian_stats"
  <div>
   <% @comedians.each do |comedian| %>
     <ul>
      <li><%=comedian.name%></li>
      <li><%=comedian.city%></li>
      <li><%=comedian.age%></li>
      <% comedian.specials.each do |special| %>
      <ul>
        <li><%=special.name%></li>
        <li><%=special.run_time%></li>
        <img src=<%=special.image_url%> /> 
      </ul>
      <% end %>
     </ul>
  <% end %>
  <div>
 </div>
</div>
